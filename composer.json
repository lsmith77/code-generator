{
  "name": "spryker/code-generator",

  "require": {
    "php": ">=5.6.0",
    "spryker/console": "*",
    "spryker/kernel": "*",
    "spryker/transfer": "*",
    "zendframework/zend-filter": "*"
  },
  "require-dev": {
    "codeception/codeception": "*",
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "description": "CodeGenerator bundle",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Test": "tests/"
    },
    "psr-4": {
      "Transfer\\Module\\": "vendor/spryker/transfer/tests/_support/Module"
    }
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ]
}
